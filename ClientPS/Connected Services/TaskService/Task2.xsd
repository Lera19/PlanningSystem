<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="Priorities">
    <xs:restriction base="xs:string">
      <xs:enumeration value="VeryLow" />
      <xs:enumeration value="Low" />
      <xs:enumeration value="Medium" />
      <xs:enumeration value="High" />
      <xs:enumeration value="VeryHigh" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Priorities" nillable="true" type="tns:Priorities" />
  <xs:simpleType name="Statuses">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ToDo" />
      <xs:enumeration value="Ready" />
      <xs:enumeration value="InProgress" />
      <xs:enumeration value="Completed" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Statuses" nillable="true" type="tns:Statuses" />
</xs:schema>